<line
  class="wick"
  x1={{x}}
  x2={{x}}
  y1={{y1}}
  y2={{y2}}
  stroke-width=1
  stroke="black"
  style={{_candleStyle}}
  shape-rendering={{shapeRendering}}
  role={{role}}></line>
<rect
  class="candle"
  style={{_candleStyle}}
  x={{candle.x}}
  y={{y}}
  width={{candleWidth}}
  height={{candleHeight}}
  role={{role}}
  shape-rendering={{shapeRendering}}></rect>